{
  "compilerOptions": {
    "module": "module",
    "esModuleInterop": true,
    "outDir": ".",
    "rootDir": ".",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "moduleResolution": "node",
    "target": "ES6"
  },
  "include": [
    "./**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}